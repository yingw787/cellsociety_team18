<?xml version="1.0"?>
<simulation type="waTor">
	<parameters>
		<rules>
			<shark breedingRate="5" initialEnergy="3" gainEnergy="1"></shark>
			<fish breedingRate="5"></fish>
		</rules>
		<gridProperties wrap="false" numberOfGridNeighbors="4">
			<breadth>3</breadth>
			<length>3</length>
		</gridProperties>
		<colorScheme>
			<map state="0" color="#0097FF"></map>
			<map state="1" color="#FF99CC"></map>
			<map state="2" color="#FF0000"></map>
		</colorScheme>
	</parameters>

	<cellConfiguration>
		<cell>
			<location x="2" y="2"></location>
			<state>1</state>
		</cell>
	</cellConfiguration>
</simulation>