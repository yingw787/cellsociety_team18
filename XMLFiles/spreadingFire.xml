<?xml version="1.0"?>
<simulation type="spreadingFire" author="it15, rd122, yw103">
	<parameters>
		<rules>
			<catchFireProbability>0.5</catchFireProbability>	
		</rules>
		<gridProperties wrap="false" numberOfGridNeighbors="4">
			<breadth>30</breadth>
			<length>30</length>
		</gridProperties>
		<colorScheme>
			<map state="0" color="#663300"></map>
			<map state="1" color="#CC3300"></map>
			<map state="2" color="#33CC33"></map>
		</colorScheme>
	</parameters>

	<cellConfiguration>
		<cell>
			<location x="2" y="23"></location>
			<state>2</state>
			<shape>Circle</shape>
		</cell>
		<cell>
			<location x="27" y="21"></location>
			<state>1</state>
			<shape></shape>
		</cell>
	</cellConfiguration>
</simulation>